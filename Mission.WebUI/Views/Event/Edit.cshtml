@model Event
@using Mission.Domain.Entities
<script type="text/javascript" src="@Url.Content("~/Content/tinymce/jscripts/tiny_mce/tiny_mce.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Content/js/LoadTinymce.js")"></script>
<link rel="stylesheet" href="http://code.jquery.com/ui/1.9.2/themes/base/jquery-ui.css" />
<script src="http://code.jquery.com/jquery-1.8.3.js"></script>
<script src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
<div id="posts-list">

<h2 class="page-heading"><span>ÄNDRA EVENT</span></h2>

@using (Html.BeginForm()){

    
    <div>
        <p>
            Titel     <br />
           @Html.EditorFor(e => e.HeadLine, new {  cols = "30", style = "width:100%", height = "100%" })
           
        </p>
        <p>
            Information <br />           
            @Html.TextAreaFor(e => e.Description, new { id = "elm1", name = "elm1", rows = "10", cols = "30", style = "width:100%", height = "100%" })
        </p>
         <p>
            Stad <br />
            @Html.EditorFor(e => e.City)
        </p>
         <div id="to-do">
            Datum och tid <br />
                    @Html.TextBoxFor(e => e.Date, new { id = "datepicker", value = "10/10/2011 12:00 AM", style = "width:200px;", @class = "" })
         </div>
  
            

    
     <script>
         $(function () {
             $("#datepicker").datepicker();
         });
    </script>

    </div>
    <div>
        <input type="submit" value ="Spara"/>
    </div>


}</div></div>