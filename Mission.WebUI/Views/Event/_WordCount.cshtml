@model IPagedList<Mission.Domain.Entities.Words>
@using PagedList.Mvc
@using PagedList
<div id="wordlist">
    <table>
        @foreach (var word in Model)
        {
            <tr>
                <td style="text-transform: capitalize;">@word.Word</td>
                <td>@word.WordCount</td>
            </tr>
        }
    </table>
    <div id="selectPage">
        @Html.PagedListPager((IPagedList)Model, page => Url.Action("Overview", new { page }))
    </div>
</div>
