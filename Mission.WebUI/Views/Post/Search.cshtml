@model IEnumerable<Mission.Domain.Entities.SearchResult>
          
@{
    ViewBag.Title = "Search";
}

<div class="searchdiv">
    <h2>@ViewBag.StatusMessage</h2>
    <div id="posts-list" class="post-list-partial">
       @foreach (var searchObject in Model)
        {
            <article class="format-standard" id="@searchObject.ID">
	            <h2  class="post-heading"><a href="@searchObject.Url">@searchObject.Title</a></h2>
	            <div class="excerpt">@MvcHtmlString.Create(@searchObject.Excerpt)</div>
            </article>
        }
</div>
</div>