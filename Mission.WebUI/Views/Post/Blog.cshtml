﻿@using Mission.Domain.Entities
@using Mission.WebUI.ViewModels
@model BlogViewModel
           
@{
    ViewBag.Title = "Blog";
}
<div id="posts-list" class="post-lists-blogg">
    <h2 class="page-heading"><span>BLOGG</span></h2>
        @Html.Partial("_Arkiv", Model.Arkivmodel)
        @Html.DisplayFor(p => p.Blogcomments.Posts)
</div> 
@using PagedList
@using PagedList.Mvc
<div id="selectPage">
        @Html.PagedListPager((IPagedList)Model.Blogcomments.Posts, page => Url.Action("Blog", new { page }))
    </div>
